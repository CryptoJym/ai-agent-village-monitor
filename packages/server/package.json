{
  "name": "@ai-agent-village-monitor/server",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "build": "tsup",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run",
    "test:int": "USE_TESTCONTAINERS=true vitest run src/__tests__/integration.test.ts",
    "typecheck": "tsc --noEmit",
    "load:ws": "node scripts/ws-load.js",
    "load:ws:websocket": "node scripts/ws-load.js --transport=websocket",
    "load:ws:polling": "node scripts/ws-load.js --transport=polling",
    "load:scenario:polling": "node scripts/ws-load.js --transport=polling --clients=1000 --ramp=60 --time=120 --csv=load-polling.csv",
    "load:test": "node scripts/load-test.js",
    "load:run:all": "node scripts/load-runner.cjs",
    "load:k6:smoke": "k6 run load/k6-http.js",
    "load:k6:ramp": "K6_RAMP=1 k6 run load/k6-http.js",
    "load:artillery:smoke": "artillery run load/artillery-ws.yml",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:push": "prisma db push --schema=prisma/schema.prisma",
    "db:reset": "prisma migrate reset -f --schema=prisma/schema.prisma",
    "db:seed": "prisma db seed --schema=prisma/schema.prisma",
    "db:seed:load": "node prisma/seed.load.cjs",
    "db:seed:synthetic": "node prisma/seed.synthetic.cjs",
    "webhook:send": "node scripts/send-webhook.mjs"
  },
  "dependencies": {
    "@octokit/graphql": "^8.2.0",
    "@octokit/plugin-retry": "^6.0.1",
    "@octokit/plugin-throttling": "^9.3.0",
    "@octokit/rest": "^21.1.1",
    "@socket.io/redis-adapter": "^8.2.0",
    "bullmq": "^5.58.5",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "probot": "^13.3.6",
    "rate-limit-redis": "^4.2.2",
    "socket.io": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/supertest": "^6.0.3",
    "nock": "^13.5.6",
    "pg": "^8.12.0",
    "prisma": "^6.19.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "testcontainers": "^10.10.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "node prisma/seed.js"
  }
}
